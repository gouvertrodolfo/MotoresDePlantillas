extends index

block producto 

    table.table-striped
      tr
        th Nombre 
        th Precio
        th Foto


      each p in productos
        tr
          td= p.title 
          td= p.price
          td= img
              img.image(src=p.thumbnail, style="width:25px ;height:25px")

    form(action="/productos",method="get",class="form-producto")
            input(type="submit", value="Agregar producto")


    
